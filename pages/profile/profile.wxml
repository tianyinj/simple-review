<template name="review-item">
      <text class="title">{{item.title}}</text>
</template>

<template name="review-list">
  <view class="list">
    <template is="review-item" data="{{item}}" wx:for="{{my_reviews}}" wx:key="item.review_id"/>
  </view>
</template>

<template is="review-list" data="{{my_reviews}}"/>
